{
    "userName": {
        "regEx": "^[A-Za-zА-Яа-яЁё]+$",
        "msg": "Имя содержит только буквы"
    },
    "userPhone": {
        "regEx": "^\\+7\\(\\d{3}\\)\\d{3}-\\d{4}$",
        "msg": "Введите телефон в формате +7(000)000-0000"
    },
    "userEmail": {
        "regEx": "^\\w+.?\\w+@\\w+\\.\\w{2,3}$",
        "msg": "Формат email - mymail@mail.ru, или my.mail@mail.ru"
    }
}